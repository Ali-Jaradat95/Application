            <abp-column size-md="_12">
                <form id="SearchForm" autocomplete="off">
                    <div class="input-group">
                        <input class="form-control page-search-filter-text" id="FilterText" placeholder="@L["Search"]"/>
                        <abp-button button-type="Primary" type="submit" icon="search"/>
                    </div>
                </form>
            </abp-column>
            <abp-column size-md="_12" class="mt-3">
                <a href="javascript:;" id="AdvancedFilterSectionToggler" class="text-decoration-none">@L["SeeAdvancedFilters"]</a>
            </abp-column>
        </abp-row>

        <abp-row id="AdvancedFilterSection" style="display: none;">
            <abp-column size="_3">
                <abp-input asp-for="BillerCodeFilterMin" label="@L["MinBillerCode"].Value" />
            </abp-column>
            <abp-column size="_3">
                <abp-input asp-for="BillerCodeFilterMax" label="@L["MaxBillerCode"].Value" />
            </abp-column>
            <abp-column size="_3">
                <abp-input asp-for="BillingNoFilter" label="@L["BillingNo"].Value" />
            </abp-column>
            <abp-column size="_3">
                <abp-input asp-for="BillNoFilter" label="@L["BillNo"].Value" />
            </abp-column>
            <abp-column size="_3">
                <abp-input asp-for="ServiceTypeFilter" label="@L["ServiceType"].Value" />
            </abp-column>
            <abp-column size="_3">
                <abp-input asp-for="PrepaidCatFilter" label="@L["PrepaidCat"].Value" />
            </abp-column>
            <abp-column size="_3">
                <abp-input asp-for="DueAmtFilterMin" label="@L["MinDueAmt"].Value" />
            </abp-column>
            <abp-column size="_3">
                <abp-input asp-for="DueAmtFilterMax" label="@L["MaxDueAmt"].Value" />
            </abp-column>
            <abp-column size="_3">
                <abp-input asp-for="ValidationCodeFilter" label="@L["ValidationCode"].Value" />
            </abp-column>
            <abp-column size="_3">
                <abp-input asp-for="JOEBPPSTrxFilter" label="@L["JOEBPPSTrx"].Value" />
            </abp-column>
            <abp-column size="_3">
                <abp-input asp-for="BankTrxIdFilter" label="@L["BankTrxId"].Value" />
            </abp-column>
            <abp-column size="_3">
                <abp-input asp-for="BankCodeFilter" label="@L["BankCode"].Value" />
            </abp-column>
            <abp-column size="_3">
                <abp-input asp-for="PmtStatusFilter" label="@L["PmtStatus"].Value" />
            </abp-column>
            <abp-column size="_3">
                <abp-input asp-for="PaidAmtFilterMin" label="@L["MinPaidAmt"].Value" />
            </abp-column>
            <abp-column size="_3">
                <abp-input asp-for="PaidAmtFilterMax" label="@L["MaxPaidAmt"].Value" />
