@section content_toolbar {
    <abp-button id="ExportToExcelButton" text="@L["ExportToExcel"].Value" icon="download" size="Small" button-type="Primary" />
    @if (await Authorization.IsGrantedAsync(ApplicationPermissions.MadfoatcomRequests.Create))
    {
        <abp-button id="NewMadfoatcomRequestButton" text="@L["NewMadfoatcomRequest"].Value" icon="plus" size="Small" button-type="Primary" />
