            <abp-column size="_3">
                <abp-input asp-for="EmailFilter" label="@L["Email"].Value" />
            </abp-column>
@*//<suite-custom-code-block-5>*@
@*//</suite-custom-code-block-5>*@
        </abp-row>

        <abp-table striped-rows="true" id="MadfoatcomRequestsTable">
            <thead>
				<tr>
					<th>@L["Actions"]</th>
					<th>@L["BillerCode"]</th>
					<th>@L["BillingNo"]</th>
					<th>@L["BillNo"]</th>
					<th>@L["ServiceType"]</th>
					<th>@L["PrepaidCat"]</th>
					<th>@L["DueAmt"]</th>
					<th>@L["ValidationCode"]</th>
					<th>@L["JOEBPPSTrx"]</th>
