            <abp-input asp-for="MadfoatcomRequest.FeesAmt"   data-val="false"/>


            <abp-input asp-for="MadfoatcomRequest.FeesOnBiller"   />

            <abp-date-picker auto-update-input="false" today-button-classes="btn-primary" asp-for="MadfoatcomRequest.ProcessDate"  />
